{
  "env": {
    "browser": true
  },
  "extends": ["jnmorse"],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "import/prefer-default-export": "off"
  },
  "overrides": [
    {
      "files": ["webpack/**/*.js", "postcss.config.js"],
      "rules": {
        "import/no-extraneous-dependencies": "off"
      }
    }
  ]
}
